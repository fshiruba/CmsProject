@using CmsProject.Models.BaseTypes
@using EPiServer.Core
@model IBaseViewModel<IContent>

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    @if (Model.CurrentPage is BasePage basePage)
    {
        <title>@basePage.Title</title>
    }
    else
    {
        <title>@Model.CurrentPage.Name</title>
    }

    <link href="~/scss/vendors/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/CmsProject.styles.css" rel="stylesheet" />
</head>

<body>
    @RenderBody()
    <script src="~/js/vendors/jquery-3.4.0.min.js" type="text/javascript"></script>
    <script src="~/js/vendors/popper.min.js" type="text/javascript"></script>
    <script src="~/js/vendors/bootstrap-4.3.1/bootstrap.min.js" type="text/javascript"></script>
</body>

</html>